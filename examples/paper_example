initFromFile "plane.off"

%%

Parcel  --> split("x") {~1: extrude(15) Tower | ~1: GreenSpace | ~1: extrude(12) Tower} ;;

Tower --> split("y") { 2:Base | {~0.4: Floor}* } ;;

Floor --> translate(0.2, 0, 0) Floor2 ;;
Floor --> translate(-0.2, 0, 0) Floor2 ;;

Floor2 --> translate(0, 0, 0.2) ;;
Floor2 --> translate(0, 0, -0.2) ;;
